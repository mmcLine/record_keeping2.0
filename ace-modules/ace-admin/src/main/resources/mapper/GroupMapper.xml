  <delete id="deleteRoleMembersById">
      delete from base_user_role where role_id = #{role_id}
  </delete>

  <insert id="insertRoleMembersById">
    insert into base_user_role (
       role_id,user_id
    )
    values(
       #{role_id},#{userId}
    )
  </insert>
</mapper>